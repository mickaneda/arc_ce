<% @queues.each_pair do |queue_name, queue_data| %>
[queue/<%= queue_name -%>]
name="<%= queue_name -%>"
<% queue_data.each_pair do |key, val| -%>
<% if key == 'default_memory' -%>
defaultmemory="<%= val -%>"
<% elsif key == 'OSName' -%>
opsys="<%= val -%>"
OSName="<%= val -%>"
<% elsif key == 'OSVersion' -%>
opsys="<%= val -%>"
OSVersion="<%= val -%>"
<% elsif key == 'OSVersionName' -%>
opsys="<%= val -%>"
OSVersionName="<%= val  -%>"
<% elsif key == 'cpudistribution' -%>
cpudistribution=<%= val.join(',') %>
<% elsif key == 'authorized_vos' -%>
<% val.each do |vos| -%>
authorizedvo="<%= vos %>"
<% if queue_data['ac_policy'] == true -%>
ac_policy="VOMS: <%= vos %>"
<% end -%>
<% end -%>
<% elsif key == 'ac_policy' -%>
<% else  -%>
<%= key %>="<%= val %>"
<% end -%>
<% end -%>
<% end -%>
